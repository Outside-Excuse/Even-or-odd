import java.util.Scanner;
class Main {
    public static void main(String[] args) {
      Scanner scanner = new Scanner (System.in);
      
      int number = scanner.nextInt();
      
      while (number != 0 ){
          if ((number % 2 ) == 0 ){
              System.out.println("even");
          } // termina el if
      else if ((number % 2) != 0){
          System.out.println("odd");
      } // termina el if
      number = scanner.nextInt();
      
      }
      
      
        
    }
}
